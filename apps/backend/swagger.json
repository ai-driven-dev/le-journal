  "info": {
    "title": "Le Journal API",
    "description": "Documentation de l'API Le Journal",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "UserDomain": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "c123e456-789b-12d3-a456-426614174000",
            "description": "User's ID"
          },
          "email": {
            "type": "string",
            "example": "john.doe@email.com",
            "description": "User's email"
          },
          "role": {
            "type": "string",
            "example": "ADMIN",
            "description": "User's role"
          },
          "name": {
            "type": "string",
            "example": "John Doe",
            "description": "User's name"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "example": "2024-02-08T12:00:00.000Z",
            "description": "Creation date"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "example": "2024-02-09T14:30:00.000Z",
            "description": "Last update date"
          },
          "onboardingStartedAt": {
            "type": "object",
            "example": "2024-02-08T12:00:00.000Z",
            "description": "Onboarding started date"
          },
          "onboardingCompletedAt": {
            "type": "object",
            "example": "2024-02-09T14:30:00.000Z",
            "description": "Onboarding completion date"
          },
          "avatar": {
            "type": "string",
            "example": "https://example.com/avatar.png",
            "description": "User's avatar URL"
          },
          "googleId": {
            "type": "string",
            "example": "google-123",
            "description": "User's Google ID"
          }
        },
        "required": [
          "id",
          "email",
          "role",
          "name",
          "createdAt",
          "updatedAt",
          "onboardingStartedAt",
          "onboardingCompletedAt",
          "avatar",
          "googleId"
        ]
      },
      "Project": {
        "type": "object",
        "properties": {}
      },
      "ProjectUpdate": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "c123e456-789b-12d3-a456-426614174000",
            "description": "Project ID"
          },
          "promptInstruction": {
            "type": "string",
            "description": "The instruction prompt for the project",
            "example": "Write a blog post about AI and its impact on society"
          }
        },
        "required": [
          "id",
          "promptInstruction"
        ]
      },
      "SetupProjectDto": {
        "type": "object",
        "properties": {}
      }
    }
  }
}